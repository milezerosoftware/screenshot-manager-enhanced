{
  "schemaVersion": 1,
  "id": "screenshot-manager-enhanced",
  "version": "${version}",
  "name": "Screenshot Manager Enhanced",
  "description": "A advanced screenshot manager to help organize your screenshots between different worlds, dimensions, and game modes. Works in single player and multiplayer",
  "authors": [
    "Scriptmunkee"
  ],
  "contact": {
    "homepage": "https://modrinth.com/project/screenshot-manager-enhanced",
    "sources": "https://github.com/milezerosoftware/screenshot-manager",
    "issues": "https://github.com/milezerosoftware/screenshot-manager/issues"
  },
  "license": "Apache-2.0",
  "icon": "assets/screenshot-manager/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "com.milezerosoftware.mc.screenshotmanagerenhanced.ModInitializerImpl"
    ],
    "client": [
      "com.milezerosoftware.mc.screenshotmanagerenhanced.ScreenshotManagerClient"
    ],
    "modmenu": [
      "com.milezerosoftware.mc.screenshotmanagerenhanced.client.compat.ModMenuIntegration"
    ]
  },
  "mixins": [
    "screenshot-manager-enhanced.mixins.json",
    {
      "config": "screenshot-manager-enhanced.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.17.3",
    "minecraft": "~1.21.10",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}